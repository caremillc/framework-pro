{
    "name": "caremillc/framework-pro",
    "description": "Caremi PHP framework",
    "keywords": ["framework", "php", "caremi"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://caremi.com",
    "support": {
        "issues": "https://github.com/caremillc/framework-pro/issues",
        "source": "https://github.com/caremillc/framework-pro"
    },
    "authors": [
        {
            "name": "ikoncoders",
            "email": "caremillc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-session": "*",
        "ext-tokenizer": "*",
        "ext-pdo": "*", 
        "nikic/fast-route": "^1.3",
        "psr/container": "^2.0",
        "league/container": "^4.0",
        "symfony/dotenv": "^6.0",
        "twig/twig": "^3.0",
        "doctrine/dbal": "^3.6",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Careminate\\": "Careminate/",
            "Database\\": "databases/migrations/"
        },
        "files": [
            "Careminate/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Careminate\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    }
}